              $(document).ready(function(){

              		// 선택한 정보 표시
              		initSelected();

              		// 선택 처리
              		$('.mask_vote').click(function(){

              		    //goProfile($(this).data("id"));
              		    //return;

              			if(poll_info=='' || poll_info=='null'){
              				alert("현재 투표기간이 아닙니다.");
              				return false;
              			}

              			if( voteComplete ){
              				alert("오늘 이미 투표를 완료하셨습니다. 내일 다시 참여해주세요.");
              				return false;
