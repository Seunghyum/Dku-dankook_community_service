              		if(listLength > 98){$(".listnb.nb_07").show();}

                      $(window).scroll(function() {
              			scrollAction();
              			scrollActionAd();
              			if($(window).scrollTop() > 50) {
              				$(".top").fadeIn(150);
              			}else{
              				$(".top").fadeOut(150);
              			}
              		});


              		var _rnb = $('#qmset');
              		var _topBtn = $(".top");

              		var headerHeight = $('#header').outerHeight()+115;
              		var defaultTop = 62;
              		var lastChkNum = $(document).height() - (_rnb.height() + $('#footerGlabal_2016').outerHeight() );
              		var whNum = $(window).height();


              		function scrollAction(){
              			if(_rnb.length == 0 ) return false;
              			var num = $(window).scrollTop();

              			if(num > headerHeight && num < lastChkNum){
              				//_rnb.css('top', num - (headerHeight - defaultTop));
              				_rnb.css('top', 0);
              				_rnb.addClass("fixed");
              			}else if(num <= headerHeight ){
              				_rnb.css('top', defaultTop);
              				_rnb.removeClass("fixed");
              			}else{
              				_rnb.css('top', lastChkNum - (headerHeight - defaultTop));
              				_rnb.removeClass("fixed");
              			}
              		}
              		$(".top").fadeOut(0);
              		$(".top").css("position", "absolute");


              		//lnb 배너
                  	var _lnbAd = $('.ADmainLeft');
                  	var headerHeightAd = $('#header').outerHeight()+115;
                  	var defaultTopAd = 62;
                  	var lastChkNumAd = $(document).height() - (_lnbAd.outerHeight() + $('#footerGlabal_2016').outerHeight() );

                  	function scrollActionAd()
                  	{

                  		if(_lnbAd.length == 0 ) return false;
                  		var numAd = $(window).scrollTop();

                  		if(numAd > headerHeightAd && numAd < lastChkNumAd){
                  			//_lnbAd.css('top', numAd - (headerHeightAd - defaultTopAd));
                  			_lnbAd.css('top', 0);
                  			_lnbAd.addClass("fixed");
                  		}else if(numAd <= headerHeightAd ){
                  			_lnbAd.css('top', defaultTopAd);
                  			_lnbAd.removeClass("fixed");
                  		}else{
                  			_lnbAd.css('top', lastChkNumAd - (headerHeightAd - defaultTopAd));
                  			_lnbAd.removeClass("fixed");
                  		}
                  	}

                  	scrollAction();
                  	scrollActionAd();

                  	$(window).resize(function(){
                  		whNum = $(window).height();
                  		scrollAction();
                  		scrollActionAd();
                  	})

              	});
