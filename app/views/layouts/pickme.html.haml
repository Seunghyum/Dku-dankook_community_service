              					login();
              				}else{
              					return false;
              				}
              			}else if( $(this).parent().parent().parent().hasClass("dimd") ){
              				alert("방출자는 선택이 불가합니다. 다른연습생을 선택해주세요.");
              				return false;
              			}else{
              				var li = $(this).parents('li');
              				if( $(li).hasClass('dimd') ){
              					return;
              				}
              				li.toggleClass('selected');
              				if( $(li).hasClass("selected") ){
              					if( !(setVote($(this).data("id"), $(this).data("url"), $(this).data("name") )) ){
              						li.toggleClass('selected');
              					}
              				}else{
              					delVote($(this).data("id"));
              				}
              			}
              		});

              		var listLength = $(".tranieeList ul li").length;
              		if(listLength > 14){$(".listnb.nb_01").show();}
              		if(listLength > 28){$(".listnb.nb_02").show();}
              		if(listLength > 42){$(".listnb.nb_03").show();}
              		if(listLength > 56){$(".listnb.nb_04").show();}
              		if(listLength > 70){$(".listnb.nb_05").show();}
